<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0/EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.morph.mapper.AdminUserMapper">

    <!--List-->
    <select id="list" parameterType="hashMap" resultType="org.example.morph.dto.AdminUserDto$ListResDto">
        SELECT user.id,
               user.username,
               user.deleted,
               user.created_at as createdAt,
               user.modified_at as modifiedAt
        FROM user
        WHERE user.id is not NULL
    </select>

</mapper>